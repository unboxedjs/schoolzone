{
  "name": "school",
  "version": "0.0.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start:server": "nest start --watch | bunyan -o short",
    "start:prod": "node dist/api/main",
    "start": "concurrently -c \"green.bold,blue.bold\" -n \"API,SCHOOL\" \"yarn start:server\" \"yarn start:client --progress=false --verbose=false\"",
    "format:check": "prettier --check \"backend/**/*.ts\" \"test/**/*.ts\" \"frontend/**/*.ts\"",
    "format": "prettier --write \"backend/**/*.ts\" \"test/**/*.ts\" \"frontend/**/*.ts\"",
    "doc": "compodoc -p tsconfig.client.json --output documentation/reference --silent --customFavicon ./frontend/school/src/favicon.ico --customLogo ./frontend/shared/src/assets/images/logo.png",
    "doc:serve": "compodoc -p tsconfig.client.json --output documentation/reference --silent --customFavicon ./frontend/school/src/favicon.ico --customLogo ./frontend/shared/src/assets/images/logo.png -s",
    "lint": "eslint \"{backend,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "start:client": "ng serve",
    "build:client": "ng build --prod",
    "lint:client": "ng lint",
    "test:client": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular-devkit/build-ng-packagr": "~0.1000.2",
    "@angular/animations": "~10.0.2",
    "@angular/cdk": "^10.0.1",
    "@angular/cli": "~10.0.1",
    "@angular/common": "~10.0.2",
    "@angular/compiler": "~10.0.2",
    "@angular/compiler-cli": "~10.0.2",
    "@angular/core": "~10.0.2",
    "@angular/forms": "~10.0.2",
    "@angular/google-maps": "^10.2.0",
    "@angular/material": "^10.0.1",
    "@angular/material-moment-adapter": "^10.1.2",
    "@angular/platform-browser": "~10.0.2",
    "@angular/platform-browser-dynamic": "~10.0.2",
    "@angular/router": "~10.0.2",
    "@angular/service-worker": "~10.0.2",
    "@auth0/angular-jwt": "^5.0.1",
    "@compodoc/compodoc": "^1.1.11",
    "@google-cloud/logging-bunyan": "^3.0.0",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/testing": "^7.0.0",
    "@ngrx/data": "^9.2.0",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/entity": "^9.2.0",
    "@ngrx/router-store": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@nrwl/jest": "^9.5.1",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "bcrypt": "^5.0.0",
    "bunyan": "^1.8.14",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.23.3",
    "jest": "^26.1.0",
    "jest-preset-angular": "^8.2.1",
    "moment": "^2.27.0",
    "mongoose": "^5.9.23",
    "ng-packagr": "^10.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prettier": "^1.19.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "~6.5.5",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@ngrx/schematics": "^9.2.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bunyan": "^1.8.6",
    "@types/express": "^4.17.3",
    "@types/express-rate-limit": "^5.0.0",
    "@types/helmet": "^0.0.47",
    "@types/jest": "^26.0.4",
    "@types/mongoose": "^5.7.31",
    "@types/node": "^13.9.1",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "backend",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "collectCoverage": true,
    "coverageDirectory": "../coverage/backend",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@sz/interface$": "<rootDir>/interfaces",
      "^@sz/enum$": "<rootDir>/enums",
      "^@sz/utils$": "<rootDir>/utils"
    }
  }
}
